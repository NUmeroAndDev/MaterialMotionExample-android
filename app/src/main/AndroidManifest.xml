<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.numero.motion_example">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MotionExample.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".container.ContainerTransitionActivity"
            android:label="@string/title_container_transition" />
        <activity
            android:name=".sharedaxis.SharedAxisActivity"
            android:label="@string/title_shared_axis" />
        <activity
            android:name=".fadethrough.FadeThroughActivity"
            android:label="@string/title_fade_through" />
        <activity
            android:name=".fade.FadeActivity"
            android:label="@string/title_fade" />
    </application>

</manifest>